<?xml version="1.0" encoding="utf-8"?>

<odoo>

    <!-- product.template inherit form view -->
    <record id="product_template_only_form_view_add_label_fields" model="ir.ui.view">
        <field name="name">product.template.inherit.view.form.add.label.fields</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_only_form_view"/>
        <field name="arch" type="xml">
            <field name="barcode" position="after">
                <field name="label_qty"/>
                <field name="label_uom"/>
            </field>
            
        </field>
    </record>
</odoo>
